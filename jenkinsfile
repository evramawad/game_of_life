node{
        stage ('SCM Checkout'){
          git 'https://github.com/evramawad/game_of_life'
        }
          
        stage ('Compile Stage') {
                withMaven(maven : 'maven')
                    sh 'mvn clean package'
        }

        stage ('Deployment Stage') {
             sshagent (credentials: ['tomcat-dep']) {
    sh 'scp -o StrictHostKeyChecking=no target/*.war tomcat@192.168.191.5:source/apache-tomcat-8.5.41/webapps/'
        }
}
